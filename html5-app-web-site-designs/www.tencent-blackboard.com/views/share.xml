<?xml version="1.0" encoding="UTF-8"?>
<container name="share" width="640" height="1008" clip="true">
    <asset>images/share/bg.png</asset>
    <asset>images/100yuan/sound/off.png</asset>
    <asset>images/100yuan/sound/on.png</asset>
    <asset>images/share/share/normal.jpg</asset>
    <asset>images/share/share/click.jpg</asset>

    <container name="content" width="640" height="1008">
        <bitmap name="bg" width="640" height="1008" src="images/share/bg.png"/>
        <container name="btn_share" x="159" y="871" width="232" height="68">
            <bitmap name="normal" src="images/share/share/normal.jpg"/>
            <bitmap name="click" src="images/share/share/click.jpg"/>
        </container>
    </container>

    <container name="sound_box" width="640" height="1008" muted="true">
        <container x="560" y="7" width="73" height="72">
            <movie x="10" y="10" width="53" height="52" auto-play="false">
                <frame>images/100yuan/sound/off.png</frame>
                <frame>images/100yuan/sound/on.png</frame>
            </movie>
        </container>
    </container>
</container>